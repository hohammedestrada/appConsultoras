<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="HomeNavBarTitleText" parent="FontLatoRegular">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:lineSpacingExtra">0.2sp</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="HomeNavBarSubTitleText" parent="FontLatoRegular">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#A0000000</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:lineSpacingExtra">0.2sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="HomeMenuItemText" parent="FontLatoRegular">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#AA000000</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:lineSpacingExtra">0.2sp</item>
        <item name="android:gravity">start|center_vertical</item>
    </style>

    <style name="HomeMenuItemLatText" parent="FontLatoBold">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#AA000000</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:lineSpacingExtra">0.2sp</item>
        <item name="android:gravity">start|center_vertical</item>
    </style>

    <style name="HomeMenuGridItemText" parent="FontLatoRegular">
        <item name="android:textSize">9sp</item>
        <item name="android:textColor">#c01d1d26</item>
        <item name="android:layout_marginStart">5dp</item>
        <item name="android:layout_marginEnd">5dp</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="HomeMenuSubItemText" parent="FontLatoRegular">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#44000000</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:lineSpacingExtra">0.2sp</item>
        <item name="android:gravity">start|center_vertical</item>
    </style>
