<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="StockoutsTab">
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingStart">10dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingEnd">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="StockoutsHeaderTitle" parent="FontLatoRegular">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:lineSpacingExtra">0.2sp</item>
        <item name="android:gravity">start</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingStart">15dp</item>
        <item name="android:paddingEnd">15dp</item>
    </style>

    <style name="StockoutsContent">
        <item name="android:paddingTop">16dp</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:layout_marginEnd">16dp</item>
    </style>


    <style name="StockoutsFilterContent">
        <item name="android:background">@drawable/shape_edittext_round</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginBottom">16dp</item>
    </style>

    <style name="StockoutsFilterText" parent="FontLatoLight">
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:gravity">start</item>
        <item name="android:textColor">@color/text_filter</item>
        <item name="android:textSize">16sp</item>
        <item name="android:background">@drawable/shape_edittext_no_bottom_line</item>
        <item name="android:inputType">textCapWords</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="StockoutsListNullText" parent="FontLatoBold">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/text_primary</item>
        <item name="android:textSize">14sp</item>
        <item name="android:padding">@dimen/padding_text</item>
    </style>

    <style name="StockoutsItem">
        <item name="android:background">#fff</item>
    </style>

    <style name="StockoutsItemSlash">
        <item name="android:paddingStart">25dp</item>
        <item name="android:paddingLeft">25dp</item>
        <item name="android:paddingRight">25dp</item>
        <item name="android:paddingEnd">25dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">5dp</item>
    </style>

    <style name="StockoutsTitleText" parent="FontLatoRegular">
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/text_primary</item>
        <item name="android:textSize">16sp</item>
        <item name="android:padding">@dimen/padding_text</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="StockoutsItemDataTitle" parent="FontLatoBold">
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">start</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textSize">15sp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
    </style>

    <style name="StockoutsItemDataDescription" parent="FontLatoLight">
        <item name="android:textColor">@color/item_text_description</item>
        <item name="android:gravity">start</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textSize">12sp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>
</resources>
